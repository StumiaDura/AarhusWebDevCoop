  <homepage id="1057" key="0e313f94-dc2a-4153-8c70-2f41c30f01a6" parentID="-1" level="1" creatorID="0" sortOrder="0" createDate="2018-03-21T12:23:37" updateDate="2018-03-23T18:20:16" nodeName="Homepage" urlName="homepage" path="-1,1057" isDoc="" nodeType="1052" creatorName="Lukas Klinga" writerName="Lukas Klinga" writerID="0" template="1051" nodeTypeAlias="homepage" isPublished="true">
