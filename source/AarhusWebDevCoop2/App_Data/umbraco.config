    <articlesMain id="1073" key="be21dabb-d385-4b07-9b98-7236717c7025" parentID="1057" level="2" creatorID="0" sortOrder="1" createDate="2018-03-21T13:29:20" updateDate="2018-03-21T21:53:59" nodeName="News" urlName="news" path="-1,1057,1073" isDoc="" nodeType="1069" creatorName="Lukas Klinga" writerName="Lukas Klinga" writerID="0" template="1068" nodeTypeAlias="articlesMain" isPublished="true">
